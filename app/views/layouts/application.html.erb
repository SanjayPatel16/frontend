<% content_for :content do %>
  <a class="skip-to-link" href="#main">Skip to main content</a>
  <a class="skip-to-link" href="#primary-nav">Skip to main navigation</a>

  <%= render 'shared/opt_out_bar' unless dismissed_beta_opt_out? %>
  <%= render 'shared/alerts' if alerts? %>
  <%= render 'shared/header' %>

  <%= content_for?(:main) ? yield(:main) : yield %>

  <%= render 'shared/contact' %>
  <%= render 'shared/footer' %>
<% end %>

<%= render template: 'layouts/base' %>
